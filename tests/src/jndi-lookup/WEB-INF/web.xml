<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app>
	<servlet>
		<servlet-name>jndi_lookup</servlet-name>
		<servlet-class>org.jboss.tomcat.test.jndilookup.JndiLookupServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>jndi_lookup</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>

	<env-entry>
		<env-entry-name>boolean</env-entry-name>
		<env-entry-value>true</env-entry-value>
		<env-entry-type>java.lang.Boolean</env-entry-type>
	</env-entry>
</web-app>
